#SERIE DE DIFERENCIAL UNIVERSIDAD EQUIVALENTE/SECUNDARIA EQUIVALENTE Y OFERTA RELATIVA
#gnuplot unihsrelsup relwuni --time-series --with-lines { set title 'Serie Diferencial Salarial y Oferta Relativa Universidad Equivalente/Secundaria Equivalente'; } --output=display 
#Modelo 1: diferencial univ. equiv/secundaria equivalente ~ constante+log-of. rel.
ols relwuni 0 l_unihsrelsup
series uh1 = $uhat 
fcast 1991 2014 f1
setinfo uh1 --description="Residuos de modelo 1" --graph-name=""
normtest uh1 --jbera
corrgm uh1 12
#Modelo 2: diferencial univ. equiv/secundaria equivalente ~ constante+log-of. rel.+tendencia lineal
ols relwuni 0 l_unihsrelsup ano
